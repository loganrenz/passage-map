{
  "name": "passage-map",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "./scripts/dev-all.sh",
    "dev:all": "./scripts/dev-all.sh",
    "dev:nuxt": "nuxt dev",
    "dev:nuxt-only": "doppler run -- nuxt dev",
    "dev:wrangler": "doppler run -- wrangler pages dev .output/public --compatibility-date=2025-12-01",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "nuxt typecheck",
    "wrangler": "wrangler",
    "r2:create": "wrangler r2 bucket create",
    "r2:list": "wrangler r2 bucket list",
    "r2:delete": "wrangler r2 bucket delete",
    "r2:dev": "wrangler dev",
    "d2:api:deploy": "cd workers/d2-api && wrangler deploy",
    "d2:api:dev": "cd workers/d2-api && wrangler dev --port 8787"
  },
  "dependencies": {
    "@apple/mapkit-loader": "^0.2.1",
    "@aws-sdk/client-s3": "^3.956.0",
    "@influxdata/influxdb-client": "^1.35.0",
    "@nuxt/ui": "^4.3.0",
    "@types/jsonwebtoken": "^9.0.10",
    "jsonwebtoken": "^9.0.3",
    "nuxt": "^4.2.2",
    "vue": "^3.5.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251219.0",
    "@iconify-json/lucide": "^1.2.81",
    "@nuxt/eslint": "^1.12.1",
    "@types/apple-mapkit-js-browser": "^5.78.1",
    "@types/node": "^20.10.0",
    "concurrently": "^9.2.1",
    "typescript": "^5.2.0",
    "wrangler": "^4.56.0"
  }
}
